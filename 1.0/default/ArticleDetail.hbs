            <span class="material-icons w3-jumbo">
                schedule
            </span>
        </div>
        <div class="w3-col m10">

            <div class="w3-row w3-small w3-padding-small">
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-06" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-0}} - {{genxml.data.genxml.textbox.close1-0}} - {{genxml.data.genxml.textbox.open2-0}} - {{genxml.data.genxml.textbox.close2-0}}</div>
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-07" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-1}} - {{genxml.data.genxml.textbox.close1-1}} - {{genxml.data.genxml.textbox.open2-1}} - {{genxml.data.genxml.textbox.close2-1}}</div>
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-08" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-2}} - {{genxml.data.genxml.textbox.close1-2}} - {{genxml.data.genxml.textbox.open2-2}} - {{genxml.data.genxml.textbox.close2-2}}</div>
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-09" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-3}} - {{genxml.data.genxml.textbox.close1-3}} - {{genxml.data.genxml.textbox.open2-3}} - {{genxml.data.genxml.textbox.close2-3}}</div>
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-10" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-4}} - {{genxml.data.genxml.textbox.close1-4}} - {{genxml.data.genxml.textbox.open2-4}} - {{genxml.data.genxml.textbox.close2-4}}</div>
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-11" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-5}} - {{genxml.data.genxml.textbox.close1-5}} - {{genxml.data.genxml.textbox.open2-5}} - {{genxml.data.genxml.textbox.close2-5}}</div>
                <div class="w3-col m2 w3-right-align" style="width:100px;">{{formatDateTime "2022-02-12" "dddd"}}:&nbsp;</div>
                <div class="w3-col m10">{{genxml.data.genxml.textbox.open1-6}} - {{genxml.data.genxml.textbox.close1-6}} - {{genxml.data.genxml.textbox.open2-6}} - {{genxml.data.genxml.textbox.close2-6}}</div>
            </div>

        </div>
    </div>

    <div class="w3-row w3-padding">
        <h3>Contact / Accès</h3>
        <div class="w3-row">
            <div class="w3-col m3">
                <img class="imgrollovertarget" alt="{{article this "name"}}" src="{{article this "imageurl" 0 180 180}}" style="width:180px;">
            </div>

            <div class="w3-col m9">
                <div>
                    <strong>{{article this "name"}}</strong><br />
                    {{genxml.data.genxml.textbox.address1}}<br />
                    {{genxml.data.genxml.textbox.address2}}<br />
                    {{genxml.data.genxml.textbox.address3}}<br />
                    {{genxml.data.genxml.textbox.address4}}<br />
                </div>
                <div>
                    {{resourcekey this "Theme.phone"}}: {{genxml.data.genxml.textbox.phone}}
                </div>
                <div>
                    {{resourcekey this "Theme.email"}}: {{protectemail genxml.data.genxml.textbox.email genxml.catalogsettings.genxml.textbox.catalogname genxml.data.genxml.textbox.email}}
                </div>
                <div>
                    <b>
                        {{resourcekey this "Theme.website"}}:
                        <a href="{{genxml.data.genxml.textbox.website}}" target="_blank">
                            {{genxml.data.genxml.textbox.website}}
                        </a>
                    </b>
                </div>
            </div>
        </div>
    </div>

    <div class="w3-row">
        <div class="w3-row">
            <h5>Social Networks</h5>
        </div>
        {{#when genxml.data.genxml.checkbox.youtubehide "eq" "true"}}
