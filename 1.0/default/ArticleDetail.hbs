            <a href="{{genxml.data.genxml.checkbox.linkedinlink}}" target="_blank">
                <img style="width:40px;" class="w3-circle w3-grey w3-grayscale-max" src="{{genxml.sessionparams.r.engineurl}}/DesktopModules/RocketThemes/RocketAppThemes-W3-CSS/rocketcatalog.BusinessDirectory/1.0/img/linkedin.png" />
                <span>LinkedIn</span>
            </a>
        </div>
        {{/when}}

        {{#when genxml.data.genxml.checkbox.instagramhide "eq" "true"}}
        <div class="w3-col m2 w3-padding">
            <a href="{{genxml.data.genxml.checkbox.instagramlink}}" target="_blank">
                <img style="width:40px;" class="w3-circle w3-grey w3-grayscale-max" src="{{genxml.sessionparams.r.engineurl}}/DesktopModules/RocketThemes/RocketAppThemes-W3-CSS/rocketcatalog.BusinessDirectory/1.0/img/instagram.png" />
                <span>Instagram</span>
            </a>
        </div>
        {{/when}}

        {{#when genxml.data.genxml.checkbox.twitterhide "eq" "true"}}
        <div class="w3-col m2 w3-padding">
            <a href="{{genxml.data.genxml.checkbox.twitterlink}}" target="_blank">
                <img style="width:40px;" class="w3-circle w3-grey w3-grayscale-max" src="{{genxml.sessionparams.r.engineurl}}/DesktopModules/RocketThemes/RocketAppThemes-W3-CSS/rocketcatalog.BusinessDirectory/1.0/img/twitter.png" />
                <span>Twitter</span>
            </a>
        </div>
        {{/when}}

        {{#when genxml.data.genxml.checkbox.facebookhide "eq" "true"}}
        <div class="w3-col m2 w3-padding">
            <a href="{{genxml.data.genxml.checkbox.facebooklink}}" target="_blank">
                <img style="width:40px;" class="w3-circle w3-grey w3-grayscale-max" src="{{genxml.sessionparams.r.engineurl}}/DesktopModules/RocketThemes/RocketAppThemes-W3-CSS/rocketcatalog.BusinessDirectory/1.0/img/facebook.png" />
                <span>Facebook</span>
            </a>
        </div>
        {{/when}}

    </div>

    <div class="w3-row">
        <div class="w3-half w3-padding">
            <h3>{{resourcekey this "Theme.links"}}</h3>
            <div id="linklistcontainer w3-padding">
                {{#each genxml.data.genxml.linklist.genxml}}
                <div class="w3-row">
                    {{#when checkbox.hidearticlelink "noteq" "true"}}
                    {{#when checkbox.blankarticlelink "eq" "true"}}
                    <a target="_blank" href="{{article @root "linkurl" @index}}" class="w3-bar-item w3-button">{{article @root "linkname" @index}}</a>
                    {{/when}}
                    {{#when checkbox.blankarticlelink "noteq" "true"}}
                    <a href="{{article @root "linkurl" @index}}" class="w3-bar-item w3-button">{{article @root "linkname" @index}}</a>
                    {{/when}}
                    {{/when}}
                </div>
                {{/each}}
            </div>
        </div>
        <div class="w3-half">
            <h3>{{resourcekey this "Theme.documents"}}</h3>
            <div id="linklistcontainer w3-padding">
                {{#each genxml.data.genxml.lang.genxml.documentlist.genxml}}
                {{#when checkbox.documenthiddenarticledoc "noteq" "true"}}
                <div class="w3-row">
                    <a target="_blank" href="{{article @root "docurl" @index}}" class="w3-bar-item w3-button">{{article @root "docname" @index}}</a>
                </div>
                {{/when}}
                {{/each}}
            </div>
        </div>
    </div>


    <div class="w3-row">
        <div id="leafletmap" style="height:300px" class="w3-light-grey"></div>
    </div>

</div>

    <script>

        $(document).ready(function () {

            setTimeout(() => { initmap(); }, 500);

        });


    function initmap() {

        var x = '{{genxml.data.genxml.textbox.lat}}';
        var y = '{{genxml.data.genxml.textbox.long}}';
        var defaultLat = '{{genxml.remotemodule.genxml.settings.lat}}';
        var defaultLong = '{{genxml.remotemodule.genxml.settings.long}}';
        var companyname = '{{genxml.data.genxml.textbox.companyname}}';
        var marker = "";
        var map = "";
        if (x != '' && y != '') {
            map = L.map('leafletmap', { minZoom: 3 }).setView([x, y], 13);
            marker = L.marker([x, y], { title: companyname });
            map.addLayer(marker);
        }
        else {
            map = L.map('leafletmap', { minZoom: 3 }).setView([defaultLat, defaultLong], 13);
        }

        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            attribution: '&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors'
        }).addTo(map);

    }


    </script>

    {{! displayData this}}

